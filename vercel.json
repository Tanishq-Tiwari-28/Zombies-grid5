{  
  "version" : 2,
  "builds": [
    {
      "src": "Zombies/wsgi.py",
      "use": "@vercel/python",
      "config" : { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Zombies/wsgi.py"
    },
    {
      "env": {
        "SECRET_KEY": "@SECRET_KEY",
        "DEBUG": "@DEBUG"
      }
    }
  ]
  
}

