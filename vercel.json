{  
  "builds": [
    {
      "src": "Zombies/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Zombies/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "zombie_app.settings",
    "SECRET_KEY": "$SECRET_KEY",
    "DEBUG": "$DEBUG"
  }
  
}

