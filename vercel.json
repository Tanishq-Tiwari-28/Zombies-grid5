  {  
    "builds": [
      {
        "src": "Zombies/wsgi.py",
        "use": "@vercel/python"    
      },
      {
        "src": "build.sh",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "staticfiles/static"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "Zombies/wsgi.py"
      }
    ]
  }

